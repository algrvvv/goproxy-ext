{
    "manifest_version": 3,
    "name": "goproxy connector",
    "version": "1.1",
    "permissions": [
        "proxy",
        "storage",
        "activeTab",
        "scripting"
    ],
    "background": {
        "service_worker": "service_worker.js"
    },
    "action": {
        "default_popup": "popup.html",
        "default_icon": "icon.png"
    }
}
